{
  "name": "united-states-digital-corp",
  "version": "1.0.0",
  "description": "United States Digital Corps",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve --livereload",
    "test-links": "bundle exec htmlproofer  --url-swap 'digitalcorps.gsa.gov:localhost/:4000' _site;",

    "start-detached": "bundle exec jekyll serve --detach",
    "pa11y-ci:home": "pa11y-ci http://localhost:4000",
    "pa11y-ci:sitemap": "pa11y-ci --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude \"/*.pdf\""
  },
  "author": "U.S. Digital Corps",
  "repository": {
    "type": "git",
    "url": "https://github.com/GSA/digitalcorps.gsa.gov"
  },
  "bugs": {
    "url": "https://github.com/GSA/digitalcorps.gsa.gov/issues"
  },
  "license": "CC0-1.0",
  "dependencies": {
    "uswds": "~2.12.0"
  },
  "devDependencies": {
    "pa11y-ci": "^2.4.2",
    "rimraf": "^3.0.2"
  }
}
